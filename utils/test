int main() {
    static_cast<bool (*)(clock_t ret, int err)>([](clock_t ret, int err){ retrun ret == -1; });
    static_cast<bool (*)(clock_t ret, int )>([](clock_t ret, int ){ return (ret == -1); });
    static_cast<bool (*)(int err)>([](int err){ return errno != EINTR; });
}
